{
  "compilerOptions": {
    "jsx": "react",
    "lib": [
      "es2015",
      "dom"
    ],
    "esModuleInterop": true,
    "module": "es2015",
    "moduleResolution": "node",
    "target": "es2015",
    "skipLibCheck": true,
    "strict": true,
    "strictNullChecks": false,
    "importHelpers": true,
    "composite": true,
    "sourceMap": true,
    "declarationMap": true
  },
  "exclude": [
    "**/**.test.tsx",
    "**/**.test.ts",
    "**/examples/**"
  ]
}
