    "@patternfly/react-core": "^3.113.0",
    "@patternfly/react-icons": "^3.14.13",
    "@patternfly/react-inline-edit-extension": "^2.11.79",
    "@patternfly/react-styled-system": "^3.6.40",
    "@patternfly/react-styles": "^3.5.28",
    "@patternfly/react-table": "^2.22.28",
    "@patternfly/react-tokens": "^2.6.31",
    "@patternfly/react-topology": "^2.8.73",
    "@patternfly/react-virtualized-extension": "^1.2.80",
    "gatsby": "^2.16.4",
    "gatsby-source-filesystem": "^2.1.22",
    "gatsby-theme-patternfly-org": "^0.0.15",
    "react": "^16.8.0",
    "react-dom": "^16.8.0",
    "react-helmet": "^5.2.1"
