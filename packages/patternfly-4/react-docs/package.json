{
  "name": "@patternfly/react-docs",
  "description": "PatternFly React Docs",
  "version": "3.24.0",
  "author": "Red Hat",
  "license": "MIT",
  "publishConfig": {
    "access": "public"
  },
  "dependencies": {
    "@patternfly/react-charts": "^2.2.2",
    "@patternfly/react-core": "^2.10.0",
    "@patternfly/react-icons": "^3.6.1",
    "@patternfly/react-inline-edit-extension": "^1.1.0",
    "@patternfly/react-styled-system": "^2.0.17",
    "@patternfly/react-styles": "^2.4.0",
    "@patternfly/react-table": "^1.4.0",
    "@patternfly/react-tokens": "^2.2.1",
    "babel-standalone": "^6.26.0",
    "clipboard-copy": "^2.0.1",
    "css-loader": "^2.1.1",
    "emotion": "^9.2.9",
    "exenv": "^1.2.2",
    "gatsby": "^2.1.29",
    "gatsby-plugin-react-helmet": "^3.0.8",
    "gatsby-plugin-typescript": "2.0.3",
    "gatsby-source-filesystem": "^2.0.23",
    "gatsby-transformer-json": "^2.1.8",
    "gatsby-transformer-react-docgen": "3.0.2",
    "param-case": "^2.1.1",
    "react-helmet": "^5.2.0",
    "react-icons": "^3.5.0",
    "react-live": "^1.12.0",
    "typescript": "3.2.2"
  },
  "devDependencies": {
    "fs-extra": "^7.0.1",
    "glob": "^7.1.3",
    "rimraf": "^2.6.2",
    "typedoc": "^0.14.2"
  },
  "keywords": [
    "gatsby"
  ],
  "scripts": {
    "clean": "rimraf .cache .tmp dist public static ts-docs",
    "develop": "yarn gen-ts-docs && gatsby develop",
    "build:docs": "node build/copyDocs.js && yarn gen-ts-docs && yarn build:gatsby",
    "build:gatsby": "env NODE_OPTIONS=--max_old_space_size=4096 gatsby build --verbose --prefix-paths",
    "gen-ts-docs": "typedoc --mode file --json build/ts-output.json --tsconfig ../react-core/tsconfig.base.json ../react-core"
  },
  "browserslist": [
    "last 2 versions",
    "not ie <= 11"
  ]
}
