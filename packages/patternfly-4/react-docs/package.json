{
  "name": "@patternfly/react-docs",
  "description": "PatternFly React Docs",
  "version": "3.24.4",
  "author": "Red Hat",
  "license": "MIT",
  "private": true,
  "scripts": {
    "clean": "rimraf .cache public static",
    "build:static": "shx cp -r ../react-core/dist/styles static",
    "develop": "yarn clean && yarn build:static && gatsby develop",
    "build:docs": "yarn clean && yarn build:static && gatsby build --prefix-paths",
    "serve": "gatsby serve --prefix-paths"
  },
  "dependencies": {
    "@patternfly/react-core": "^2.10.4",
    "@patternfly/react-icons": "^3.7.0",
    "gatsby": "^2.3.3",
    "gatsby-plugin-react-helmet": "^3.0.11",
    "gatsby-source-filesystem": "^2.0.28",
    "gatsby-transformer-remark": "^2.3.8",
    "react": "~16.8.4",
    "react-dom": "~16.8.4",
    "react-helmet": "^5.2.0",
    "react-live": "^2.0.0",
    "rehype-react": "^3.1.0"
  },
  "devDependencies": {
    "rimraf": "^2.6.3",
    "shx": "^0.3.2"
  },
  "keywords": [
    "gatsby"
  ],
  "browserslist": [
    "last 2 versions",
    "not ie <= 11"
  ]
}