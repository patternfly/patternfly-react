{
  "name": "@patternfly/react-docs",
  "description": "PatternFly React Docs",
  "version": "4.17.2",
  "author": "Red Hat",
  "license": "MIT",
  "private": true,
  "scripts": {
    "build:docs": "yarn build:static && yarn build:ram --prefix-paths",
    "build:ram": "node --max-old-space-size=4096 ./node_modules/.bin/gatsby build",
    "build:static": "shx cp -r ../react-core/dist/styles/* static",
    "clean": "rimraf .cache public static/assets static/base.css",
    "clean:mdx": "rimraf .cache/caches/gatsby-mdx",
    "develop": "gatsby develop",
    "serve": "gatsby serve",
    "serve:prefix": "gatsby serve --prefix-paths",
    "test:a11y": "patternfly-a11y --file .cache/fullscreenPages.json --prefix http://localhost:9000/patternfly-4 --aggregate --noIncomplete",
    "test:a11y:ci": "start-server-and-test serve:prefix http://localhost:9000/patternfly-4 test:a11y"
  },
  "dependencies": {
    "@mdx-js/mdx": "^1.1.5",
    "@mdx-js/react": "^1.1.5",
    "@patternfly/patternfly": "2.56.2",
    "@patternfly/react-charts": "^5.2.19",
    "@patternfly/react-core": "^3.133.1",
    "@patternfly/react-icons": "^3.14.37",
    "@patternfly/react-inline-edit-extension": "^2.15.2",
    "@patternfly/react-styled-system": "^3.7.25",
    "@patternfly/react-styles": "^3.6.24",
    "@patternfly/react-table": "^2.25.2",
    "@patternfly/react-tokens": "^2.7.23",
    "@patternfly/react-topology": "^2.12.1",
    "@patternfly/react-virtualized-extension": "^1.3.89",
    "gatsby": "2.18.6",
    "gatsby-cli": "2.8.15",
    "gatsby-theme-patternfly-org": "^1.2.19",
    "react": "^16.8.0",
    "react-dom": "^16.8.0",
    "react-helmet": "^5.2.1"
  },
  "devDependencies": {
    "@patternfly/patternfly-a11y": "^0.0.11",
    "rimraf": "^2.6.3",
    "shx": "^0.3.2",
    "start-server-and-test": "^1.5.0"
  },
  "keywords": [
    "gatsby"
  ],
  "browserslist": [
    "last 2 versions",
    "not ie <= 11"
  ]
}
