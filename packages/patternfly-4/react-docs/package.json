{
  "name": "@patternfly/react-docs",
  "description": "PatternFly React Docs",
  "version": "4.16.15",
  "author": "Red Hat",
  "license": "MIT",
  "private": true,
  "scripts": {
    "build:docs": "yarn build:static && node --max-old-space-size=4096 ./node_modules/.bin/gatsby build --prefix-paths",
    "build:static": "shx cp -r ../react-core/dist/styles/* static",
    "clean": "rimraf .cache public static/assets static/base.css",
    "clean:mdx": "rimraf .cache/caches/gatsby-mdx",
    "develop": "gatsby develop",
    "serve": "gatsby serve --prefix-paths"
  },
  "dependencies": {
    "@mdx-js/mdx": "^1.1.5",
    "@mdx-js/react": "^1.1.5",
    "@patternfly/patternfly": "2.40.6",
    "@patternfly/react-catalog-view-extension": "^1.1.10",
    "@patternfly/react-charts": "^5.1.5",
    "@patternfly/react-core": "^3.120.7",
    "@patternfly/react-icons": "^3.14.18",
    "@patternfly/react-inline-edit-extension": "^2.12.21",
    "@patternfly/react-styled-system": "^3.7.5",
    "@patternfly/react-styles": "^3.6.5",
    "@patternfly/react-table": "^2.24.21",
    "@patternfly/react-tokens": "^2.7.5",
    "@patternfly/react-topology": "^2.11.7",
    "@patternfly/react-virtualized-extension": "^1.3.20",
    "gatsby": "^2.16.4",
    "gatsby-source-filesystem": "^2.1.22",
    "gatsby-theme-patternfly-org": "^0.0.16",
    "gatsby-transformer-json": "^2.2.15",
    "react": "^16.8.0",
    "react-dom": "^16.8.0",
    "react-helmet": "^5.2.1"
  },
  "devDependencies": {
    "rimraf": "^2.6.3",
    "shx": "^0.3.2"
  },
  "keywords": [
    "gatsby"
  ],
  "browserslist": [
    "last 2 versions",
    "not ie <= 11"
  ]
}
