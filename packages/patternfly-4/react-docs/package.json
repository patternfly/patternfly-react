{
  "name": "@patternfly/react-docs",
  "description": "PatternFly React Docs",
  "version": "4.9.9",
  "author": "Red Hat",
  "license": "MIT",
  "private": true,
  "scripts": {
    "build:docs": "yarn build:static && gatsby build --prefix-paths",
    "build:static": "shx cp -r ../react-core/dist/styles/* static",
    "clean": "rimraf .cache public static/assets static/base.css",
    "clean:mdx": "rimraf .cache/caches/gatsby-mdx",
    "develop": "yarn build:static && gatsby develop",
    "serve": "gatsby serve --prefix-paths"
  },
  "dependencies": {
    "@mdx-js/mdx": "1.0.2",
    "@mdx-js/react": "1.0.2",
    "@patternfly/patternfly": "2.23.0",
    "@patternfly/react-core": "^3.78.0",
    "@patternfly/react-icons": "^3.10.14",
    "@patternfly/react-virtualized-extension": "^1.1.88",
    "gatsby": "2.3.14",
    "gatsby-mdx": "0.6.3",
    "gatsby-plugin-react-helmet": "^3.0.11",
    "gatsby-plugin-sass": "^2.0.11",
    "gatsby-source-filesystem": "^2.0.28",
    "gatsby-transformer-json": "^2.1.11",
    "react": "~16.8.4",
    "react-dom": "~16.8.4",
    "react-helmet": "^5.2.0",
    "react-live": "^2.0.0"
  },
  "devDependencies": {
    "rimraf": "^2.6.3",
    "shx": "^0.3.2"
  },
  "keywords": [
    "gatsby"
  ],
  "browserslist": [
    "last 2 versions",
    "not ie <= 11"
  ]
}
