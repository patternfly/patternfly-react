{
  "presets": ["../.babelrc.js"],
  "env": {
    "production:esm": {
      "plugins": [
        [
          "@patternfly/react-styles/babel",
          {
            "srcDir": "./src",
            "outDir": "./dist/esm",
            "useModules": true
          }
        ]
      ]
    },
    "production:cjs": {
      "plugins": [
        [
          "@patternfly/react-styles/babel",
          {
            "srcDir": "./src",
            "outDir": "./dist/js",
            "useModules": false
          }
        ]
      ]
    }
  }
}
