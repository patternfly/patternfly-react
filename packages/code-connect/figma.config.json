{
  "codeConnect": {
    "parser": "react",
    "include": [
      "components/Slider/*.tsx",
      "components/Spinner/*.tsx",
      "components/Tabs/*.tsx",
      "components/Timestamp/*.tsx",
      "components/ToggleGroup/*.tsx",
      "components/Tooltip/*.tsx"
    ],
    "paths": {
      "src/components": "src/components"
    },
    "aliases": {
      "@patternfly/react-core": "."
    },
    "importPaths": {
      "src/components": "src/components"
    },
    "options": {
      "instanceSwapper": {
        "enabled": true
      },
      "development": {
        "enabled": true,
        "verbose": true
      },
      "production": {
        "enabled": false
      }
    }
  }
}