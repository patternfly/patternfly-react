{
  "name": "@patternfly-five/react-docs",
  "description": "PatternFly React Docs",
  "version": "1.0.0",
  "publishConfig": {
    "access": "public",
    "tag": "alpha-v5"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/patternfly/patternfly-react.git"
  },
  "author": "Red Hat",
  "license": "MIT",
  "scripts": {
    "analyze": "yarn build:docs --analyze",
    "build:docs": "pf-docs-framework build all",
    "clean": "rimraf .cache public static/assets static/base.css src/generated/**/*.js",
    "develop": "pf-docs-framework start",
    "serve": "pf-docs-framework serve public",
    "screenshots": "pf-docs-framework screenshots --urlPrefix http://localhost:5000",
    "test:a11y": "patternfly-a11y --config patternfly-a11y.config"
  },
  "dependencies": {
    "@patternfly-five/patternfly": "1.1.1",
    "@patternfly-five/react-catalog-view-extension": "^1.0.0",
    "@patternfly-five/react-charts": "^1.0.0",
    "@patternfly-five/react-code-editor": "^1.0.0",
    "@patternfly-five/react-core": "^1.0.0",
    "@patternfly-five/react-icons": "^1.0.0",
    "@patternfly-five/react-inline-edit-extension": "^1.0.0",
    "@patternfly-five/react-log-viewer": "^1.0.0",
    "@patternfly-five/react-styles": "^1.0.0",
    "@patternfly-five/react-table": "^1.0.0",
    "@patternfly-five/react-tokens": "^1.0.0",
    "@patternfly-five/react-topology": "^1.0.0",
    "@patternfly-five/react-virtualized-extension": "^1.0.0"
  },
  "devDependencies": {
    "@patternfly/documentation-framework": "1.2.44",
    "@patternfly/patternfly-a11y": "4.3.1",
    "rimraf": "^2.6.3",
    "shx": "^0.3.2"
  },
  "keywords": [
    "gatsby"
  ],
  "browserslist": [
    "last 2 versions",
    "not ie <= 11"
  ]
}
